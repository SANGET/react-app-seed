{
  "name": "react-app-seed",
  "version": "0.2.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "yarn desktop:dev",
    "desktop:dev": "PLATFORM=desktop PORT=7788 node scripts/start.js",
    "mobile:dev": "PLATFORM=mobile PORT=7799 node scripts/start.js",
    "build": "ukectl build",
    "initWrokspace": "node ./scripts/init-workspace",
    "init": "git init; npm i; npm run setAppVersion",
    "setAppVersion": "ukectl setVersion",
    "injectVersionToTmpl": "node ./version/inject-version-to-tmpl.js",
    "autoZip": "npm run setAppVersion; npm run build; npm run zipBuild;",
    "update": "yarn add uke-request ukelli-ui -W",
    "zipBuild": "mkdir -p ./zip-build; zip -r ./zip-build/build.zip ./build/*",
    "test": "ukectl test"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "uke-scripts": "^0.4.19"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "basic-helper": "^1.4.x",
    "mousetrap": "^1.6.2",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.5.10",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.3.11",
    "uke-request": "^0.13.6",
    "ukelli-ui": "^2.12.3",
    "unistore": "^3.2.1"
  },
  "author": "SANGET",
  "repository": {
    "type": "git",
    "url": "https://github.com/SANGET/react-app-seed.git"
  }
}
